{
	// Place your snippets for latex here. Each snippet is defined under a snippet name and has a prefix, body and 
	// description. The prefix is what is used to trigger the snippet and the body will be expanded and inserted. Possible variables are:
	// $1, $2 for tab stops, $0 for the final cursor position, and ${1:label}, ${2:another} for placeholders. Placeholders with the 
	// same ids are connected.
	// Example:
	// "Print to console": {
	// 	"prefix": "log",
	// 	"body": [
	// 		"console.log('$1');",
	// 		"$2"
	// 	],
	// 	"description": "Log output to console"
	// }
	"marginnote": {
		"prefix": "mar",
		"body": [
			"\\marginnote{$1}",
			"$2"
		],
		"description": "Creat marginnote"
	},
	"noindent": {
		"prefix": "noi",
		"body": [
			"\\noindent $1",
		],
		"description": "noindent"
	},
	"Definition": {
		"prefix": "BDE",
		"body": [
			"\\begin{Definition}",
			"    $1",
			"\\end{Definition}",
		],
		"description": "definition"
	},
	"Theorem": {
		"prefix": "BTH",
		"body": [
			"\\begin{Theorem}",
			"    $1",
			"\\end{Theorem}",
		],
		"description": "definition"
	},
	"Enumerate": {
		"prefix": "BENUM",
		"body": [
			"\\begin{enumerate}",
			"    \\item $1",
			"\\end{enumerate}",
		],
		"description": "enumerate"
	},
	"Example": {
		"prefix": "BEXM",
		"body": [
			"\\begin{Example}",
			"    $1",
			"\\end{Example}",
		],
		"description": "example"
	},
	"MathNotation": {
		"prefix": "MAT",
		"body": [
			"\\[$1\\]$2"
		],
		"description": "Math notation",
	},
	"InlineMathNotation": {
		"prefix": "IMAT",
		"body": [
			"$ $1 $$2"
		],
		"description": "Inline Math notation",
	},
	"Omega": {
		"prefix": "@O",
		"body": [
			"\\Omega$1"
		],
		"description": "Big omega",
	},
	"AND": {
		"prefix": "@and",
		"body": [
			"\\wedge$1"
		],
		"description": "and",
	},
	"OR": {
		"prefix": "@or",
		"body": [
			"\\vee$1"
		],
		"description": "or",
	},
	"Rightarrow": {
		"prefix": "@raw",
		"body": [
			"\\Rightarrow$1"
		],
		"description": "rightarrow",
	},
	"LeftRightarrow": {
		"prefix": "@lraw",
		"body": [
			"\\Leftrightarrow$1"
		],
		"description": "leftrightarrow",
	},
	"emptyset": {
		"prefix": "@emts",
		"body": [
			"\\emptyset$1"
		],
		"description": "emptysets",
	},
}